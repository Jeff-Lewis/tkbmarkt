<p id="notice"><%= notice %></p>

<h1>Listing Mratings</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mratings.each do |mrating| %>
      <tr>
        <td><%= mrating.status %></td>
        <td><%= link_to 'Show', mrating %></td>
        <td><%= link_to 'Edit', edit_mrating_path(mrating) %></td>
        <td><%= link_to 'Destroy', mrating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mrating', new_mrating_path %>
