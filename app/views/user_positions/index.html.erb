<p id="notice"><%= notice %></p>

<h1>Listing User Positions</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Geo address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_positions.each do |user_position| %>
      <tr>
        <td><%= user_position.user_id %></td>
        <td><%= user_position.latitude %></td>
        <td><%= user_position.longitude %></td>
        <td><%= user_position.geo_address %></td>
        <td><%= link_to 'Show', user_position %></td>
        <td><%= link_to 'Edit', edit_user_position_path(user_position) %></td>
        <td><%= link_to 'Destroy', user_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User position', new_user_position_path %>
