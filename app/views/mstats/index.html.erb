<p id="notice"><%= notice %></p>

<h1>Listing Mstats</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mstats.each do |mstat| %>
      <tr>
        <td><%= mstat.status %></td>
        <td><%= link_to 'Show', mstat %></td>
        <td><%= link_to 'Edit', edit_mstat_path(mstat) %></td>
        <td><%= link_to 'Destroy', mstat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mstat', new_mstat_path %>
